{% macro configWarning(setting) -%}
  {% set setting = '<code>' ~ setting ~ '</code>' %}
  {{ 'This is being overridden by the {setting} config setting in your {file} config file.'|t('sentry', {
      setting: setting,
      file:    'sentry.php'
  })|raw }}
{%- endmacro %}

{% from _self import configWarning %}
